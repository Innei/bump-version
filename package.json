{
  "name": "@innei/bump-version",
  "type": "module",
  "bin": {
    "bump": "dist/index.cjs",
    "vv": "dist/index.cjs"
  },
  "version": "0.3.1",
  "files": [
    "dist",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "dev": "temir src/main.ts",
    "prebuild": "rm -rf dist",
    "build": "temir build src/main.ts && ncc build dist/main.cjs -o dist"
  },
  "bump": {
    "before": [
      "git pull --rebase",
      "pnpm i",
      "npm run build"
    ],
    "after": [],
    "publish": true,
    "changelog": true
  },
  "devDependencies": {
    "@innei/eslint-config-ts": "0.9.4",
    "@vercel/ncc": "0.34.0",
    "inquirer": "^9.1.2",
    "semver": "^7.3.7",
    "zx": "^7.0.8"
  },
  "dependencies": {
    "@temir/cli": "^0.0.20",
    "@temir/core": "^0.0.20",
    "@temir/select-input": "^0.0.20",
    "changelogithub": "0.12.4",
    "vue": "^3.2.39"
  },
  "resolutions": {
    "chalk": "^4.0"
  }
}